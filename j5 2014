total_chores = input()#number of minutes to do chores
total_chores = int(total_chores)
time = int(input()) #number of chores
chores_add = []

for i in range(time):
    chore_time = input()
    chore_time = int(chore_time)
    chores_add.append(chore_time)

sorted = sorted(chores_add)
total = 0
counter = 0
for i in sorted:  
   total = total + i
   counter +=1
   if total > total_chores:
       removing = total - i
       counter= counter- 1
       
   if total == total_chores:
       print(counter)
   if total < total_chores and total + i < total_chores :
       print(counter+1)
       break

    
#8 -> time 4 -> chores  == 1 3 9 8







